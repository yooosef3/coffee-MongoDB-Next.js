  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "components/products/product/productInfo/SliderInfo.js", "components/products/product/productInfo/Info.js", "components/products/product/productInfo/Slider.js", "components/home/handPicked/SliderTwo.jsx", "pages/_app.tsx", "components/layout/Header.jsx", "pages/products/[productId].jsx", "components/cart/ProductsTable.jsx", "components/cart/Total.jsx", "components/cart/Product.jsx"],
  "exclude": ["node_modules"]
}
